    "eslint-plugin-vue": "9.25.0",
    "file-saver": "2.0.5",
    "fs-extra": "11.2.0",
    "json5": "2.2.3",
    "jszip": "3.10.1",
    "lint-staged": "15.2.7",
    "localforage": "1.10.0",
    "naive-ui": "2.38.2",
    "normalize.css": "8.0.1",
    "p-limit": "5.0.0",
    "pinia": "2.1.7",
    "prettier": "3.3.2",
    "qrcode": "1.5.3",
    "sass": "1.77.6",
    "simple-git": "3.25.0",
