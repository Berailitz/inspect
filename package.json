    "vue-i18n": "9.13.1",
    "vue-router": "4.4.0",
    "vue-tsc": "2.0.24"
  },
  "volta": {
    "node": "20.12.2"
  },
  "packageManager": "pnpm@9.4.0",
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx}": [
      "prettier --cache --write",
